{
  "manifest_version": 3,
  "name": "Wikit",
  "version": "1.0",
  "description": "This extension tracks your overall spendings.",
  "icons": {
    "16": "icon16.png"
  },
  "action": {
    "default_icon": "icon16.png"
  },
  "permissions": [
    "storage",
    "notifications",
    "contextMenus",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  }
}

